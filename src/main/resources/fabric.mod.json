{
	"schemaVersion": 1,
	"id": "rubymod",
	"version": "${version}",
	"name": "Ruby Mod",
	"description": "Adds Rubies to Minecraft",
	"authors": [
		"Plixel4"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "ARR",
	"icon": "assets/rubymod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.ppatel.rubymod.RubyMod"
		],
		"client": [
			"com.ppatel.rubymod.RubyModClient"
		],
		"fabric-datagen": [
			"com.ppatel.rubymod.RubyModDataGenerator"
		]
	},
	"mixins": [
		"rubymod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.14",
		"minecraft": "~1.19.3",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}